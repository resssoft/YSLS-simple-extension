{
  "name": "Save URL",
  "version": "0.0.1.5",
  "manifest_version": 2,
  "description": "YSLS extension",
  "homepage_url": "http://resager.ru/projects/ysls",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "tabs",
    "http://localhost:8091/*"
  ]
}